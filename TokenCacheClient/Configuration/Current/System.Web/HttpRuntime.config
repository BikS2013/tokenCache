<?xml version="1.0"?>
<httpRuntime targetFramework="4.5.1" 
             enableVersionHeader="false" 
             requestValidationMode="4.5.1"
             executionTimeout="30"
             minFreeThreads="64" 
             minLocalRequestFreeThreads="64"
             appRequestQueueLimit="5000"
             maxRequestLength="51200"/>
<!--enableHeaderChecking  defaults to true and should never be disabled to avoid http response splitting attacks-->